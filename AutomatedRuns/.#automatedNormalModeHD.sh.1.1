set noclobber

if (! -e normalModeHD.m) then
    ln -s ~/Mist/Matlab/AutomatedRuns/normalModeHD.m .
endif

#cd ~/WorkDir/3GB1/1HEZ/Bidirectional

set p = 1;

while ($p <= 176)
    echo $p
@   q = $p + 10;
